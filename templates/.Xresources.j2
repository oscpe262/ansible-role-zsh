! {{ ansible_managed }} - role: zsh
*background: #262626
*foreground: #C5C8C6
! black
*color0: #262626
*color8: #373B41
!*color8: #c5c8c6
! red
*color1: #A54242
*color9: #CC6666
! green
*color2: #8C9440
*color10: #B5BD68
! yellow
*color3: #DE935F
*color11: #F0C674
! blue
*color4: #5F819D
*color12: #81A2BE
! magenta
*color5: #85678F
*color13: #B294BB
! cyan
*color6: #5E8D87
*color14: #8ABEB7
! white
*color7: #c5c8c6
*color15: #C5C8C6

 URxvt*scrollBar: false
 URxvt*cursorUnderline: false
 URxvt*colorUL: #ff88ff
 URxvt*internalBorder:0
 URxvt*borderColor: #262626
 URxvt*perl-ext-common: vtwheel,font-size
 URxvt.background: #262626

 URxvt.keysym.C-Up:       perl:font-size:increase
 URxvt.keysym.C-Down:     perl:font-size:decrease
 URxvt.keysym.C-S-Up:     perl:font-size:incglobal
 URxvt.keysym.C-S-Down:   perl:font-size:decglobal

URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.depth: 32

!URxvt.font: xft:Ubuntu Mono:size=10:normal:antialias=true:hinting=true, xft:Inconsolata for Powerline
!URxvt.boldFont: xft:Ubuntu Mono:size=10:bold:antialias=true:hinting=true, xft:Inconsolata for Powerline
URxvt.font: xft:Ubuntu\ Mono\ derivative\ Powerline:size=10:normal:antialias=true:hinting=true, xft:Inconsolata for Powerline
URxvt.boldFont: xft:Ubuntu\ Mono\ derivative\ Powerline:size=10:bold:antialias=true:hinting=true, xft:Inconsolata for Powerline
!URxvt.font: xft:Droid Sans Mono Dotted for Powerline:size=10:medium:antialias=true:hinting=true, xft:Inconsolata for Powerline
!URxvt.font: xft:Source\ Code\ Pro\ for\ Powerline:size=11:antialias=true:hinting=true, xft:Inconsolata for Powerline
!URxvt.boldFont: xft:Source\ Code\ Pro\ for\ Powerline:size=11:bold:antialias=true:hinting=true, xft:Inconsolata for Powerline:bold
!URxvt.letterSpace: -.75

rofi.lines: 15
rofi.fuzzy: true
rofi.font: Source\ Code\ Pro Semibold 10

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
!						     text			mark	   marktext
! Color scheme for normal row
rofi.color-normal:                   argb:00000000,  #C5C8C6 , argb:00000000,  #CC6666 ,  #C5C8C6
! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000,  #F99157 , argb:00000000,  #F99157 ,  #1d1f21
! Color scheme for active row
rofi.color-active:                   argb:00000000,  #f0c674 , argb:00000000,  #F0C674 ,  #1d1f21
! Color scheme window
rofi.color-window:                   argb:ee222222,  #CC6666 , #CC6666
! Separator style (none, dash, solid)
rofi.separator-style:                dash
